{
  "name": "crypto-price",
  "version": "1.0.0",
  "description": "basic crypto app showing btc price",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Crypto-Price --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Crypto-Price\"",
    "package-linux": "electron-packager . Crypto-Price --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "crypto",
    "price",
    "basic",
    "electron-app"
  ],
  "author": "Nigel Jaure <jaurenigel@gmail.com> (https://nigel.creativelab.co.zw)",
  "license": "MIT",
  "devDependencies": {
    "electron": "2.0.6",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}